[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Cypher Developers
	owner = group user/Aaron (SuperFes)
[access "refs/for/refs/*"]
	read = group Cypher Developers
	push = group Cypher Developers
	label-Code-Review = -2..+2 group Cypher Developers
	label-Verified = -1..+1 group Cypher Developers
	rebase = group Cypher Developers
	removeReviewer = group Cypher Developers
	submit = group Cypher Developers
[access "refs/heads/*"]
	read = group Cypher Developers
	create = group Cypher Developers
	push = +force group Cypher Developers
	pushMerge = group Cypher Developers
[access "refs/tags/*"]
	push = +force group user/Aaron (SuperFes)
	pushTag = group user/Aaron (SuperFes)
	pushSignedTag = group user/Aaron (SuperFes)
